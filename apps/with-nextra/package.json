{
  "name": "with-nextra",
  "private": true,
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "packageManager": "pnpm@6.30.0",
  "scripts": {
    "browserlist": "pnpx browserslist@latest --update-db -y --quiet",
    "build": "next build",
    "check": "ncu",
    "dev": "next dev --port 3003",
    "lint": "next lint",
    "performance": "lighthouse --output-path=./_lighthouse.html --view --save-assets https://mnopi.com",
    "start": "next start",
    "telemetry": "next telemetry disable",
    "upgrade": "ncu -u"
  },
  "dependencies": {
    "@octokit/core": "^3.5.1",
    "@octokit/rest": "^18.12.0",
    "lighthouse": "^9.3.1",
    "next": "^12.0.10",
    "nextra": "^1.1.0",
    "nextra-theme-blog": "^0.1.6",
    "nextra-theme-docs": "^1.2.6",
    "prism-react-renderer": "^1.3.1",
    "prismjs": "^1.26.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "shaurl": "^1.1.0",
    "swr": "^1.2.1",
    "tailwindcss": "^1.5.0",
    "webpack": "^5.68.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.39",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
}
